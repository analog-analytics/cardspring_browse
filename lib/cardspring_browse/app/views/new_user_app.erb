<h2>New User app</h2>

<form action="<%= post_url %>" method="post">
  <div class="field">
    <label for="app_id">App Id</label>
    <input  id="app_id" type="text" name="app[app_id]" />
  </div>

  <div class="field">
    <label for="context">Context</label>
    <input  id="context" type="text" name="app[context]" />
  </div>

  <input type="submit" />
</form>

<a href="<%= back_url %>">Back</a>
